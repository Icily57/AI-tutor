#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/HP ELITEBOOK 840 G5/Desktop/AI tutor/node_modules/.pnpm/@flydotio+dockerfile@0.7.10/node_modules/@flydotio/dockerfile/node_modules:/mnt/c/Users/HP ELITEBOOK 840 G5/Desktop/AI tutor/node_modules/.pnpm/@flydotio+dockerfile@0.7.10/node_modules/@flydotio/node_modules:/mnt/c/Users/HP ELITEBOOK 840 G5/Desktop/AI tutor/node_modules/.pnpm/@flydotio+dockerfile@0.7.10/node_modules:/mnt/c/Users/HP ELITEBOOK 840 G5/Desktop/AI tutor/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/HP ELITEBOOK 840 G5/Desktop/AI tutor/node_modules/.pnpm/@flydotio+dockerfile@0.7.10/node_modules/@flydotio/dockerfile/node_modules:/mnt/c/Users/HP ELITEBOOK 840 G5/Desktop/AI tutor/node_modules/.pnpm/@flydotio+dockerfile@0.7.10/node_modules/@flydotio/node_modules:/mnt/c/Users/HP ELITEBOOK 840 G5/Desktop/AI tutor/node_modules/.pnpm/@flydotio+dockerfile@0.7.10/node_modules:/mnt/c/Users/HP ELITEBOOK 840 G5/Desktop/AI tutor/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@flydotio/dockerfile/index.js" "$@"
else
  exec node  "$basedir/../@flydotio/dockerfile/index.js" "$@"
fi
